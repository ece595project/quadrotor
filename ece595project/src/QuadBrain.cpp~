#include "ros/ros.h"

int main (int argc, char** argv)
{
	ros::init(argc, argv, "QuadBrain");

	if (argc < 1)
	{
		char* cmd_vel = argv[0];
		ROS_INFO("Taking control of command velocity: [%s]", cmd_vel);			
	}

	ros::loop_rate(60); //Running at 60 hertz
	while(ros::ok())
	{
		ros::spinOnce();
		loop_rate.sleep();
	}

	return 0;
}
